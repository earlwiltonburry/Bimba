                    }))
                },
                _sortDataSource: function(n, i) {
                    var f = this,
                        o = f.options.sortable,
                        s = f.dataSource,
                        e, h, r = s.sort() || [];
                    if (n.hasClass(u) && o && o.allowUnsort !== !1 ? (n.removeClass(u), i = t) : n.addClass(u), o === !0 || o.mode === "single") r = [{
                        field: f.field,
                        dir: i
                    }];
                    else {
                        for (e = 0, h = r.length; e < h; e++)
                            if (r[e].field === f.field) {
                                r.splice(e, 1);
                                break
                            }
                        r.push({
                            field: f.field,
                            dir: i
                        })
                    }
                    s.sort(r)
                },
                _columns: function() {
