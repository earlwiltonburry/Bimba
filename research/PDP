        at = it.extend({
            init: function(n) {
                var t = this;
                r(t, n);
                it.fn.init.call(t)
            },
            done: function() {
                return this.timePassed() >= this.duration
            },
            timePassed: function() {
                return Math.min(this.duration, new Date - this.startDate)
            },
            moveTo: function(n) {
                var t = this,
                    i = t.movable;
                t.initial = i[t.axis];
                t.delta = n.location - t.initial;
                t.duration = typeof n.duration == "number" ? n.duration : 300;
                t.tick = t._easeProxy(n.ease);
                t.startDate = new Date;
                t.start()
            },
            _easeProxy: function(n) {
                var t = this;
                return function() {
                    t.movable.moveAxis(t.axis, n(t.timePassed(), t.initial, t.delta, t.duration))
                }
            }
        });
        r(at, {
            easeOutExpo: function(n, t, i, r) {
                return n == r ? t + i : i * (-Math.pow(2, -10 * n / r) + 1) + t
            },
            easeOutBack: function(n, t, i, r, u) {
                return u = 1.70158, i * ((n = n / r - 1) * n * ((u + 1) * n + u) + 1) + t
            }
        });
