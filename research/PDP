            }
        };
        bi = {
            sum: function(n, t, i) {
                return (n || 0) + i.get(t)
            },
            count: function(n) {
                return (n || 0) + 1
            },
            average: function(n, t, i, r, u) {
                return n = (n || 0) + i.get(t), r == u - 1 && (n = n / u), n
            },
            max: function(n, t, i) {
