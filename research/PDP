        name: "Column Menu",
        category: "framework",
        depends: ["popup", "filtermenu", "menu"],
        advanced: !0
    }),
    function(n, t) {
        function g(t) {
            return n.trim(t).replace(/&nbsp;/gi, "")
        }
        var i = window.kendo,
            a = i.ui,
            e = n.proxy,
            d = n.extend,
