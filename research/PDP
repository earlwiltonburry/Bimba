            }
    }

    function gt(n, t) {
        n == t.top && (t.top = n.prev);
        n == t.bottom && (t.bottom = n.next);
        n.next && (n.next.prev = n.prev);
        n.prev && (n.prev.next = n.next)
    }

    function br(n, t) {
        t.top !== n && (gt(n, t), n.next = null, n.prev = t.top, t.top.next = n, t.top = n)
    }

    function kr(n, t) {
        t.bottom !== n && (gt(n, t), n.next = t.bottom, n.prev = null, t.bottom.prev = n, t.bottom = n)
    }

    function dr(n, t, i) {
        gt(n, i);
        t == i.top && (i.top = n);
        t.next && (t.next.prev = n);
        n.next = t.next;
        n.prev = t;
        t.next = n
    }

    function gr(n, t, i) {
        gt(n, i);
        t == i.bottom && (i.bottom = n);
        t.prev && (t.prev.next = n);
        n.prev = t.prev;
        t.prev = n;
        n.next = t
    }

