    n.easing_formulas = {
        linear: function(n) {
            return n
        },
        "<": function(n) {
            return l(n, 3)
        },
        ">": function(n) {
            return l(n - 1, 3) + 1
        },
        "<>": function(n) {
            return (n *= 2, n < 1) ? l(n, 3) / 2 : (n -= 2, (l(n, 3) + 2) / 2)
        },
        backIn: function(n) {
            var t = 1.70158;
            return n * n * ((t + 1) * n - t)
        },
        backOut: function(n) {
            n -= 1;
            var t = 1.70158;
            return n * n * ((t + 1) * n + t) + 1
        },
        elastic: function(n) {
            if (n == 0 || n == 1) return n;
