                empty: s("")
            },
            pt = {
                aria: function(n) {
                    var t = "";
                    return (n.items || n.content || n.contentUrl) && (t += g + "='" + (n.expanded ? "true" : "false") + "' "), n.enabled === !1 && (t += vt + "='true'"), t
                },
