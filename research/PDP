            },
            destroy: function() {
                var n = this;
                o.fn.destroy.call(n);
                n._afterEnd();
                n.userEvents.destroy()
            },
            _afterEnd: function() {
                var t = this;
                t.hint && t.hint.remove();
                delete y[t.options.group];
                t.trigger("destroy");
                n(l).off(tt, t.captureEscape)
            }
        });
        i.ui.plugin(c);
        i.ui.plugin(vt);
        i.ui.plugin(yt);
