            for (s = i._data(n, "fxshow") || i._data(n, "fxshow", {}), ("hidden" in s) && (c = s.hidden), v && (s.hidden = !c), c ? i(n).show() : h.done(function() {
                    i(n).hide()
                }), h.done(function() {
                    var t;
                    i._removeData(n, "fxshow");
                    for (t in y) i.style(n, t, y[t])
