                        hasSubgroups: n.length > 1,
                        aggregates: f.aggregate(r.aggregates)
                    }
                })), i
            },
            groupBy: function(n) {
                if (k(n) || !this.data.length) return new u([]);
                for (var t = n.field, o = this._sortForGrouping(t, n.dir || "asc"), c = i.accessor(t), s, r = c.get(o[0], t), f = {
