            },
            remove: function(n) {
                var t, i = this,
                    r = i._isServerGrouped();
                return this._eachItem(i._data, function(u) {
                    return t = vu(u, n), t && r ? (t.isNew && t.isNew() || i._destroyed.push(t), !0) : void 0
                }), n
            },
            sync: function() {
                var t = this,
                    i, f, e = [],
                    o = [],
                    s = t._destroyed,
                    u = t._flatData(t._data),
                    r;
                if (t.reader.model) {
