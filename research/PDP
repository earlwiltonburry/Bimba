                        },
                        end: function(n) {
                            n.preventDefault()
                        }
                    })
                }
            }),
            ri = v.transitions.prefix + "Transform",
            nt, at, c, vt, yt;
        nt = v.hasHW3D ? function(n, t, i) {
            return "translate3d(" + n + "px," + t + "px,0) scale(" + i + ")"
        } : function(n, t, i) {
