            },
            group: function(n, t) {
                n = rt(n || []);
                t = t || this.data;
                var f = this,
                    i = new u(f.data),
                    r;
                return n.length > 0 && (r = n[0], i = i.groupBy(r).select(function(i) {
                    var f = new u(t).filter([{
                        field: i.field,
                        operator: "eq",
                        value: i.value,
                        ignoreCase: !1
                    }]);
