                        e._trigger(p, r(t, {
                            dropTarget: n(e.targetElement)
                        }))
                    }
                    i._trigger(d, r(t, {
                        dropTarget: n(u)
                    }));
                    e = r(i, {
                        targetElement: u
                    })
                });
                i._trigger(ut, t);
                i.hint && i._updateHint(t)
            },
            _end: function(t) {
                var i = this;
                i._withDropTarget(t, function(i, u) {
                    i && (i._drop(r({}, t, {
                        dropTarget: n(u)
                    })), e = null)
                });
                i._trigger(ft, t);
                i._cancel(t.event)
            },
            _cancel: function() {
                var n = this;
                n._activated = !1;
                n.hint && !n.dropped ? setTimeout(function() {
