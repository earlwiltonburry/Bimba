                    if (u = i.at(f + r), u === t) break;
                    e.push(u)
                }
                return e
            },
            total: function() {
                return this._total
            },
            destroy: function() {
                this.buffer.destroy();
                this.unbind()
            }
        });
        e(!0, i.data, {
            readers: {
                json: ti
            },
