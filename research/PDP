                        expand: {
                            effects: {}
                        },
                        collapse: {
                            hide: !0,
                            effects: {}
                        }
                    })
                }
            });
        i(p, {
            renderItem: function(n) {
                n = i({
                    panelBar: {},
                    group: {}
                }, n);
