                },
                focus: function() {
                    this._focusin()
                },
                _adjust: function(n) {
                    var f = this,
                        u = f.options,
                        t = u.min,
                        r = u.max;
                    return n === i ? n : (t !== i && n < t ? n = t : r !== i && n > r && (n = r), n)
                },
                _arrows: function() {
