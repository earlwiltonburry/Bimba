            depends: ["mobile.scroller"]
        }]
    }),
    function(n, t) {
        function ni(n, t) {
            var i;
            if (n === null && t !== null || n !== null && t === null || (i = n.length, i !== t.length)) return !1;
            while (i--)
                if (n[i] !== t[i]) return !1;
            return !0
        }

