            r = ".kendoButton",
            s = "disabled",
            y = "k-state-disabled",
            h = "k-state-focused",
            c = "k-state-selected",
            p = o.extend({
                init: function(n, t) {
                    var f = this;
                    o.fn.init.call(f, n, t);
                    n = f.wrapper = f.element;
                    t = f.options;
                    n.addClass(l).attr("role", "button");
                    t.enable = t.enable && !n.attr(s);
                    f.enable(t.enable);
