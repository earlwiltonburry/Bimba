                }, 100))
            },
            _rangeExists: function(n, t) {
                for (var f = this, r = f._ranges, i = 0, u = r.length; i < u; i++)
                    if (r[i].start <= n && r[i].end >= t) return !0;
                return !1
            }
        });
        ri = {};
