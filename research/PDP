                    var t = n.getFullYear();
                    return new i(t - t % 100, n.getMonth(), n.getDate())
                },
                last: function(n) {
                    var t = n.getFullYear();
                    return new i(t - t % 100 + 99, n.getMonth(), n.getDate())
                },
                compare: function(n, t) {
                    return st(n, t, 100)
                },
                setDate: function(n, t) {
                    vi(n, t, 10)
                },
                toDateString: function(n) {
                    var t = n.getFullYear();
                    return t - t % 10 + "/0/1"
                }
            }]
        };
