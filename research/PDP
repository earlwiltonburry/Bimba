                        },
                        gesturechange: function(n) {
                            var h = t.gesture,
                                c = h.center,
                                l = n.center,
                                r = n.distance / h.distance,
                                v = t.dimensions.minScale,
                                a = t.dimensions.maxScale,
                                e, o, s;
                            i.scale <= v && r < 1 && (r += (1 - r) * .8);
                            i.scale * r >= a && (r = a / i.scale);
                            o = i.x + t.offset.left;
                            s = i.y + t.offset.top;
                            e = {
                                x: (o - c.x) * r + l.x - o,
