                this._trigger(rt, n) ? this.userEvents.cancel() : this._activated = !0
            },
            _drag: function(t) {
                var i = this;
                t.preventDefault();
                i._withDropTarget(t, function(i, u) {
                    if (!i) {
                        e && (e._trigger(p, r(t, {
                            dropTarget: n(e.targetElement)
                        })), e = null);
                        return
